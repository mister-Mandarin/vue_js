{
  "compilerOptions": {
    "target": "es5",
    "module": "esnext",
    "baseUrl": "./",
    "moduleResolution": "node",
    "paths": {
      "@/*": ["src/*"]
    },
    "lib": ["esnext", "dom", "dom.iterable", "scripthost"],
    "jsx": "preserve"
  },
  // строчки ниже добавляю чтобы не было ошибок template
  "vueCompilerOptions": {
    "experimentalDisableTemplateSupport": true
  }
}
